<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".NavigationProfileFragment">

    <FrameLayout
        android:id="@+id/Frame_Nav_Profile"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="@color/milk"
        tools:ignore="MissingConstraints"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/Image_Nav_Profile"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/nav_profile"
        app:shapeAppearanceOverlay="@style/roundedImageView"
        android:layout_marginBottom="600dp"
        android:layout_centerInParent="true"
        android:layout_marginRight="230dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Frame_Nav_Profile"
        app:layout_constraintStart_toStartOf="@+id/Frame_Nav_Profile"
        app:layout_constraintTop_toBottomOf="@+id/Frame_Nav_Profile"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>